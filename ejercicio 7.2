#include <stdio.h>
#define TAM 5

int main(int argc, char *argv[]) {
	int sise;
	int arr[TAM];
	
	sise = 5;
	
	
	for (int i = 0; i<TAM; i++){
		printf("ingrese el valor de %d: ", i);
		scanf("%d", &sise);
		arr[i] = sise;
		
	}
	for (int i = 0; i<TAM; i++){
		int temp;
		for (int j = 0; j<TAM -1; j++){
			if(arr[j]>arr[j+1]){
				temp = arr[j];
				arr[j] = arr[j+1];
				arr[j+1]=temp;	
			}
		}
	}
	
	for (int i = 0; i<TAM; i++){
		printf("%d \n", arr[i]);
	}
	return 0;
}

